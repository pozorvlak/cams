<div id="content">
  <div id="header">
    <h1>Results</h1>
    <h2>Sets of cams to protect <% min %>mm&ndash;<% max %>mm cracks</h2>
  </div>
  <table id="results">
    <tr><th>Cams</th><th>Mass (g)</th></tr>
    <% FOREACH camset IN camsets %>
      <tr>
        <td><% camset.as_string() %></td>
        <td><% camset.mass %></td>
      </tr>
    <% END %>
  </table>
  <h2>Again! Again!</h2>
  <% INCLUDE form.tt %>
</div>
